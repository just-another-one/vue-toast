{
  "name": "@just-another-one/vue-toast",
  "sideEffects": false,
  "version": "0.1.0",
  "description": "Just another one plugin to show notifications",
  "main": "dist/vue-toast.umd.js",
  "module": "dist/vue-toast.es.js",
  "style": "dist/style.css",
  "types": "types/src/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "toast",
    "messages",
    "notifications"
  ],
  "author": "Dmytro Mashchenko <dmitriy.maschenko@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/just-another-one/vue-toast.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "postinstall": "husky install",
    "serve": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.4",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.6",
    "vite-plugin-dts": "^0.9.6",
    "vue": "^3.2.0",
    "vue-tsc": "^0.30.0"
  },
  "dependencies": {
    "@vueuse/core": "^7.4.1"
  }
}
